extend base

block content 
    main.home 
        .card-container 
            a(href="/admin/createLab")
                .card.animate__animated.animate__flipInX
                    p.card__title Create Lab

            a(href="/admin/uploadCode")
                .card.animate__animated.animate__flipInX 
                    p.card__title Upload Code

            a(href="/admin/createDomain")
                .card.animate__animated.animate__flipInX 
                    p.card__title Create Domain

            a(href="/admin/addCourse")
                .card.animate__animated.animate__flipInX 
                    p.card__title Add Course
